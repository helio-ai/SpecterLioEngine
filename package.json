{
  "name": "ai-chat-agent",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec \"tsx src/index.ts\"",
    "dev:clean": "lsof -ti:8080 | xargs kill -9 2>/dev/null || true && nodemon --watch src --ext ts --exec \"tsx src/index.ts\"",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "start": "node dist/index.js",
    "test:agent": "tsx src/agent/test/agent.test.ts",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky"
  },
  "dependencies": {
    "@langchain/anthropic": "^0.3.25",
    "@langchain/community": "^0.3.0",
    "@langchain/core": "^0.3.66",
    "@langchain/langgraph": "^0.3.0",
    "@langchain/openai": "^0.6.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/luxon": "^3.7.1",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.19.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.30",
    "luxon": "^3.7.1",
    "mongoose": "^8.0.2",
    "node-fetch": "^3.3.2",
    "openai": "^4.60.1",
    "stripe": "^14.18.0",
    "tsx": "^4.20.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.18",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2",
    "typescript-eslint": "^8.39.1"
  },
  "packageManager": "yarn@1.22.22"
}
